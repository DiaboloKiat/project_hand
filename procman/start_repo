cmd "01_core" {
    exec = "roscore";
    host = "localhost";
}

cmd "02_start_bodyparts_ros" {
    exec = "roslaunch bodyparts_ros bodyparts.launch";
    host = "localhost";
}

cmd "03_start_egohand_ros" {
    exec = "roslaunch egohands_ros egohands.launch";
    host = "localhost";
}

script "start_repo" 
{
    start cmd "01_core" wait "running";
    wait ms 1500;
}